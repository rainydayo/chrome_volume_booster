{
  "manifest_version": 3,
  "name": "Volume Booster",
  "version": "1.0.0",
  "description": "Boost the current tab's audio up to 200% with a simple slider.",
  "permissions": ["tabCapture", "offscreen", "storage", "tabs"],
  "action": {
    "default_title": "Volume Booster",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "service_worker.js",
    "type": "module"
  }
}
